
<%= link_to carnets_path do %>
  <div class="myItem1 label-center ticket-counter"
  <%#= raise %>
    data-max=" <% sum = 0  %>
    <% current_user.carnets.where('remaining_tickets > ?', 0).map do |carnet| %>
    <% sum += carnet.carnet_template.ticket_nb %>
    <% end %>
    <%= sum %>"
    data-pool="<%= current_user.pool %>"
    data-duration="1000"
    data-value="<%= current_user.carnets.where('remaining_tickets > ?', 0).sum(:remaining_tickets) %>">
  </div>
<% end %>
